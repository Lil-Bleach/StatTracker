<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".OverwatchActivity"
    tools:showIn="@layout/app_bar_overwatch_signin">

    <ImageView
        android:id="@+id/ow_rankicon"
        android:layout_width="139dp"
        android:layout_height="112dp"
        android:layout_marginStart="192dp"
        android:layout_marginBottom="222dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/ow_output"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        app:layout_constraintVertical_bias="0.879" />

    <TextView
        android:id="@+id/ow_output"
        android:layout_width="331dp"
        android:layout_height="186dp"
        android:layout_marginStart="19dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="19dp"
        android:textAlignment="viewStart"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.307"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ow_send"
        app:layout_constraintVertical_bias="0.964" />

    <EditText
        android:id="@+id/ow_input"
        android:layout_width="wrap_content"
        android:layout_height="41dp"
        android:layout_marginBottom="11dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Mastermind-1790"
        app:layout_constraintBottom_toTopOf="@+id/ow_send"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="209dp"
        android:layout_height="22dp"
        android:layout_marginTop="6dp"
        android:layout_marginBottom="65dp"
        android:text="Name-Battletag"
        app:layout_constraintBottom_toTopOf="@+id/ow_send"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/ow_send"
        android:layout_width="97dp"
        android:layout_height="0dp"
        android:layout_marginBottom="374dp"
        android:text="Submit "
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <ImageView
        android:id="@+id/ow_ppic"
        android:layout_width="139dp"
        android:layout_height="112dp"
        android:layout_marginStart="41dp"
        android:layout_marginEnd="58dp"
        android:layout_marginBottom="222dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/ow_send"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/ow_output"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        app:layout_constraintVertical_bias="0.879" />

</android.support.constraint.ConstraintLayout>